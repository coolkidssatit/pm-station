{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [
        ".env",
        "Dockerfile"
    ],
    "pipeline": {
        "dev": {
            "cache": false
        },
        "build": {
            "outputs": [
                ".next/**",
                "build/**"
            ]
        },
        "deploy": {
            "dependsOn": [
                "build"
            ],
            "outputs": []
        }
    }
}