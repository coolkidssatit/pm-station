{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [
        ".env"
    ],
    "pipeline": {
        "dev": {
            "cache": false
        },
        "prebuild": {
            "cache": false
        },
        "test": {
            "cache": false
        },
        "lint": {
            "cache": false
        },
        "build": {
            "dependsOn": [
                "^build"
            ]
        },
        "client-web#deploy": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "public/build/**",
                "build/**"
            ]
        },
        "kiosk-web#deploy": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**"
            ]
        },
        "deploy": {}
    }
}