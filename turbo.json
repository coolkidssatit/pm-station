{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [
        ".env"
    ],
    "pipeline": {
        "dev": {
            "cache": false
        },
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**",
                "public/build/**",
                "build/**",
                ".cache"
            ]
        },
        "deploy": {
            "dependsOn": [
                "build"
            ],
            "outputs": []
        }
    }
}